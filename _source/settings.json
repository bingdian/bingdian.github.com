{
  "site": {
    "name": "On The Way",
    "description": "爱折腾，爱生活...",
    "url": "http://wlog.cn",
    "canonical": true,
    "icon": false,
    "startup": false
  },
  
  

  "config": {
    "source": "content",
    "output": "../",
    "static": "../static",
    "static_prefix": "/static/",
    "permalink": "{{folder}}/{{filename}}.html",
    "relative_url": false,
    "perpage": 50,
    "feedcount": 100,
    "timezone": "+08:00"
  },

  "author": {
    "default": "bingdian",
    "vars": {
    }
  },

  "reader": {
    "active": [
      "liquidluck.readers.markdown.MarkdownReader"
    ],
    "vars": {}
  },

  "writer": {
    "active": [
      "liquidluck.writers.core.PostWriter",
      "liquidluck.writers.core.PageWriter",
      "liquidluck.writers.core.ArchiveWriter",
      "liquidluck.writers.core.ArchiveFeedWriter",
      "liquidluck.writers.core.FileWriter",
      "liquidluck.writers.core.StaticWriter",
      "liquidluck.writers.core.YearWriter",
      "liquidluck.writers.core.CategoryWriter",
      "liquidluck.writers.core.TagWriter",
      "liquidluck.writers.core.TagCloudWriter"
    ],

    "vars": {
      "archive_output": "index.html"
    }
  },

  "theme": {
    "name": "responsive",
    "lang": "zh-CN",
    "vars": {
      "lang": "zh-CN",
      "navigation": [
        {
            "link": "index.html",
            "name": "INDEX"
        },
        {
            "link": "tag/index.html",
            "name": "TAGS"
        },
        {
            "link": "about.html",
            "name": "ABOUT ME"
        },
        {
            "link": "links.html",
            "name": "LINKS"
        },
        {
            "link": "project.html",
            "name": "PROJECTS"
        },
        {
            "link": "feed.xml",
            "name": "FEED"
        }
      ],
      "analytics":"UA-1245097-16",
      "disqus": "bingdian-ontheway"
    }
  },

  "template": {
    "vars": {
    },
    "filters": {}
  }
}
